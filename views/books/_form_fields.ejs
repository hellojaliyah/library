<div>
<label>Title</label>
<input type="text" name="title" value="<%= book.title %>">
</div>
<div>
<label>Graduate</label>
<select name="graduate"> 
<% graduates.forEach(graduate => { %>
    <% if (graduate.id === book.graduate) { %>

    <option selected label="<%= graduate.name %>" value="<%= graduate.id %>">
<% } else { %>
</br>
    <option label="<%= graduate.name %>" value="<%= graduate.id %>"
    <% } %>
        <% }) %>>
</select>
</div>
</div>
<div>
    <label>Publish Date</label>
    <input type="date" name="publishDate"
          value="<%= book.publishDate == null ? '' : book.publishDate.toISOString().split('T')[0] %>">
  </div>
<div>
    <label>Page Count</label>
    <input type="number" name="pageCount" min="1"
    value="<%= book.pageCount %>">
    </div>
 <div>
    <label>Cover</label>
    <input type="file" name="cover">
</div>
<div>
    <label>Description</label>
    <textarea name="description"><%= book.description %></textarea>
</div>